{
  "plugins": ["better-docs/component","better-docs/typescript"],
  "source": {
    "include": ["./src","./backend"],
    "includePattern": ".(js|jsx|ts|tsx)$",
    "excludePattern": "(public|node_modules|public|other)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "destination": "./build/static/doc",
    "template": "node_modules/better-docs",
    "readme": "./README.jsdoc.md"
  }
}